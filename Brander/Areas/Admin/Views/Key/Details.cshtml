@model Brander.Models.Key
@{
    ViewData["Title"] = "Details";
}

<br />
<h2 class="text-white">Key Detalles</h2>
<br />

<div class="backgroundWhite rounded">
    <fieldset disabled>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Id" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Id" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Price" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Price" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <div class="col-2">
                <label asp-for="StockId" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Stock.Name" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="GameId" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Game.Name" class="form-control" />
            </div>
        </div>
    </fieldset>
    <br />
    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailsAndBackToListButton" model="Model.Id" />
        </div>
    </div>
</div>

